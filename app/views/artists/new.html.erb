<body>
  <div class = "container">
　   <p class="text">アーティスト新規登録</p>
      <div class="row">
        <div class = "main">
          <%= form_for(@artist) do |f| %>
            <p class = "text1">
            	<span class = "name"><%= f.label :アーティスト名 %></span>
            	<%= f.text_field :artist_name %>
            </p>
            <div class = "main-bottom">
                <span class = "button">
                	<%= link_to "戻る", artists_path, class:"btn btn-default btn-sm", :style=>"margin-bottom: -12px;"%>
                </span>
                	<input class="btn btn-default btn-sm" type="submit" value="送信ボタン">
            </div>
                <% end %>
        </div>
      </div>
  </div>
</body>


